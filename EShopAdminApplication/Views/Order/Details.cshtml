@model EShopAdminApplication.Models.Order
<div>
    <a class="btn btn-success">Order for: @Model.Owner.UserName</a>
</div>
<table class="table">
    <thead>
        <tr>
            <th>
                Price
            </th>
            <th>
                Concert
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
        @for (int i = 0; i < Model.ProductInOrders.Count; i++)
        {
            var item = Model.ProductInOrders.ElementAt(i).OrderedProduct;
            <tr>
                <td>
                    @Html.DisplayFor(modelItem => item.Price)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.ConcertId)
                </td>
            </tr>
        }
    </tbody>
</table>
